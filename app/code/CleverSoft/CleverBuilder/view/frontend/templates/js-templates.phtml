<script>
function searchElementFunction() {
    var input, filter, ul, li, a, i, txtValue;
    input = document.getElementById("cs-search-element-panel");
    filter = input.value.toUpperCase();
    ul = document.querySelectorAll(".layout-type-list, .widget-type-list, .element-type-list");
    for (k = 0; k < ul.length; k++) {
        li = ul[k].getElementsByTagName("li");
        for (i = 0; i < li.length; i++) {
            a = li[i].getElementsByTagName("h3")[0];
            txtValue = a.textContent || a.innerText;
            if (txtValue.toUpperCase().indexOf(filter) > -1) {
                li[i].style.display = "";
            } else {
                li[i].style.display = "none";
            }
        }
    }
}
</script>

