<script>
    require(['jquery', 'jquery/ui'], function($){
        $(document).on("click",".tab-control", function (e) {    
            e.preventDefault();
            e.stopPropagation();
            var tab = $(e.currentTarget);
            $('.tab-control').not(tab).removeClass('active');
            tab.addClass('active');

            $('.widget-form-tab').hide();
            $('.widget-form-tab[data-tab="'+tab.data('tab')+'"]').show();
            if (tab.data('tab') == 'template') {
                $('.cs-remove-layout').show();
            } else {
                $('.cs-remove-layout').hide();
            }
        });
    });
</script>